@article{gandal2012GammaSynchronyTranslational,
  title = {Gamma Synchrony: Towards a Translational Biomarker for the Treatment Resistant Symptoms of Schizophrenia},
  shorttitle = {Gamma Synchrony},
  author = {Gandal, Michael J. and Edgar, J. Christopher and Klook, Kerstin and Siegel, Steven J.},
