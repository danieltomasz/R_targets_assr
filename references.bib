@article{brake2024NeurophysiologicalBasisAperiodic,
  title = {A Neurophysiological Basis for Aperiodic {{EEG}} and the Background Spectral Trend},
  author = {Brake, Niklas and Duc, Flavie and Rokos, Alexander and Arseneau, Francis and Shahiri, Shiva and Khadra, Anmar and Plourde, Gilles},
