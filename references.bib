@article{martinez-canada2023CombiningAperiodicSlopes,
  title = {Combining Aperiodic 1/f Slopes and Brain Simulation: {{An EEG}}/{{MEG}} Proxy Marker of Excitation/Inhibition Imbalance in {{Alzheimer}}'s Disease},
  shorttitle = {Combining Aperiodic 1/f Slopes and Brain Simulation},
  author = {{Mart{\'i}nez-Ca{\~n}ada}, Pablo and {Perez-Valero}, Eduardo and Minguillon, Jesus and Pelayo, Francisco and {L{\'o}pez-Gordo}, Miguel A. and Morillas, Christian},
  year = {2023},
  journal = {Alzheimer's \& Dementia: Diagnosis, Assessment \& Disease Monitoring},
