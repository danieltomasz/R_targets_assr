@article{manting2021AuditorySteadystateResponses,
  title = {Auditory Steady-State Responses during and after a Stimulus: {{Cortical}} Sources, and the Influence of Attention and Musicality},
  shorttitle = {Auditory Steady-State Responses during and after a Stimulus},
  author = {Manting, Cassia Low and Gulyas, Balazs and Ull{\'e}n, Fredrik and Lundqvist, Daniel},
