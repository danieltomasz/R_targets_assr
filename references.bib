@article{corcoran2018ReliableAutomatedMethod,
  title = {Toward a Reliable, Automated Method of Individual Alpha Frequency ({{IAF}}) Quantification},
  author = {Corcoran, Andrew W. and Alday, Phillip M. and Schlesewsky, Matthias and {Bornkessel-Schlesewsky}, Ina},
  year = {2018},
  journal = {Psychophysiology},
  volume = {55},
