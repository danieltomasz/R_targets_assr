  langid = {english},
  annotation = {Read\_Status: New\\
Read\_Status\_Date: 2024-11-23T23:53:39.103Z},
  file = {/Users/daniel/Zotero/storage/3TNIGHCK/Donoghue - 2024 - A systematic review of aperiodic neural activity in clinical investigations.pdf}
}

@article{duma2024ExcitationInhibitionBalance,
