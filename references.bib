@article{sauer2023SpectralPhasecoherenceCorrelates,
  title = {Spectral and Phase-Coherence Correlates of Impaired Auditory Mismatch Negativity ({{MMN}}) in Schizophrenia: {{A MEG}} Study},
  shorttitle = {Spectral and Phase-Coherence Correlates of Impaired Auditory Mismatch Negativity ({{MMN}}) in Schizophrenia},
  author = {Sauer, Andreas and {Grent-'t-Jong}, Tineke and {Zeev-Wolf}, Maor and Singer, Wolf and Goldstein, Abraham and Uhlhaas, Peter J.},
  year = {2023},
  month = nov,
  journal = {Schizophrenia Research},
  volume = {261},
