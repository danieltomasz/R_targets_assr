@article{ruberg2021DetentePracticalUnderstanding,
  title = {D{\'e}tente: {{A Practical Understanding}} of {{P}} Values and {{Bayesian Posterior Probabilities}}},
  shorttitle = {D{\'e}tente},
  author = {Ruberg, Stephen J.},
