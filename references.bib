  keywords = {Behavioral neuroscience,Cognitive neuroscience,Neuroscience,notion,Physiology,Sensory neuroscience},
  file = {/Users/daniel/Zotero/storage/SJMYQIZZ/Yang - Natural brain state change with EI balance shifting toward inhibition is associated with vigilance.pdf}
}

