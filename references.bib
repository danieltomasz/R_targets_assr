  langid = {english},
  keywords = {/unread,Biophysical models,Network models,Neural circuits,notion},
  annotation = {Read\_Status: In Progress\\
Read\_Status\_Date: 2024-03-04T05:47:08.597Z},
  file = {/Users/daniel/Zotero/storage/XSYZC4EU/Brake et al. - 2024 - A neurophysiological basis for aperiodic EEG and the background spectral trend.pdf}
}

@article{brookes2022MagnetoencephalographyOpticallyPumped,
  title = {Magnetoencephalography with Optically Pumped Magnetometers ({{OPM-MEG}}): The next Generation of Functional Neuroimaging},
  shorttitle = {Magnetoencephalography with Optically Pumped Magnetometers ({{OPM-MEG}})},
