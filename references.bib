@article{wilkinson2021IncreasedAperiodicGamma,
  title = {Increased Aperiodic Gamma Power in Young Boys with {{Fragile X Syndrome}} Is Associated with Better Language Ability},
  author = {Wilkinson, Carol L. and Nelson, Charles A.},
  year = {2021},
